@model IEnumerable<DnD5E.Models.FeaturesModel>

@foreach (var item in Model)
{
    <div class="content-wrapper">
        @for (int i = 0; i < item.Description.Length; i++)
        {
            if (i == 0)
            {
                <p><b>@(item.Name):</b> @Html.Raw(item.Description[i])</p>
            }
            else
            {
                <p>@Html.Raw(item.Description[i])</p>
            }
        }
    </div>
}