@model IEnumerable<DnD5E.Models.CharacterModel>

@{
    Layout = "_Layout";
    ViewData["Title"] = "Index";
}

<h2>Choose your character</h2>

@await (Component.InvokeAsync<DnD5E.ViewComponents.CharacterListViewComponent>())

<a class="btn" href="@Url.Action("Create", "Character")" title="Create Character">Create Character</a>

<a class="btn btn-primary" href="@Url.Action("Random", "Character")" title="Create Random Character">Create Random Character</a>